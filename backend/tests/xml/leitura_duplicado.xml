<?xml version="1.0" encoding="UTF-8"?>
<estufa>
  <nome>Estufa Teste</nome>
  <localizacao>Fazenda X</localizacao>

  <sensores>
    <sensor id="S1">
      <tipo>temperatura</tipo>
      <unidade>C</unidade>
    </sensor>
  </sensores>

  <leituras>
    <!-- PRIMEIRA LEITURA -->
    <leitura>
      <dataHora>2025-10-21T10:00:00-03:00</dataHora>
      <sensorRef ref="S1"/>
      <valor>22.5</valor>
    </leitura>

    <!-- LEITURA DUPLICADA: mesmo sensorRef + mesma dataHora -->
    <leitura>
      <dataHora>2025-10-21T10:00:00-03:00</dataHora>
      <sensorRef ref="S1"/>
      <valor>23.0</valor>
    </leitura>
  </leituras>
</estufa>
